  "version": "1.0.0",
  "description": "all about testing",
  "main": "index.js",
  "browser": "./lib/browser.js",
  "scripts": {
    "test": "jest --colors --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collectCoverage --colors",
    "react-dev": "webpack --watch --mode production",
    "compress": "webpack --config webpack.config.js",
    "db:setup": "node database-mongodb/seed.js",
    "db:setup2": "node database-mongodb/seed2.js",
    "start": "node index.js",
    "dockerstart": "node index.js & node database-mongodb/seed2.js & node database-mongodb/seed2.js",
    "devjson": "webpack --mode production --profile --json > stats.json",
    "build": "webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FiveFinance/ratings_history_module.git"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "env": {
      "production": {
        "presets": [
          "minify"
        ]
      }
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FiveFinance/ratings_history_module/issues"
  },
