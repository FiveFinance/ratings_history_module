    "babel-loader": "^8.0.5",
    "brotli-gzip-webpack-plugin": "^0.5.0",
    "eslint": "^4.19.1",
    "jest": "^24.6.0",
    "jest-cli": "^24.7.1",
    "mongodb-memory-server": "^4.2.1",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.8.6",
    "supertest": "^4.0.2",
    "webpack-cli": "^3.3.0",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.4",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint-config-airbnb": "^17.1.0",
    "express": "^4.16.4",
    "faker": "^4.1.0",
    "jest-environment-jsdom": "^24.7.1",
    "moment": "^2.24.0",
