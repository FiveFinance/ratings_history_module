{
  "name": "ratings_module",
  "version": "2.0.0",
  "description": "Purchase History microservice",
  "main": "server.js",
  "browser": "./lib/browser.js",
  "scripts": {
    "test": "jest --colors --detectOpenHandles -u",
    "test:watch": "jest --watch -u",
    "test:coverage": "jest --collectCoverage --colors -u",
    "eslint": "node_modules/eslint/bin/eslint.js .",
    "eslintfix": "node_modules/eslint/bin/eslint.js --fix .",
    "stats": "set NODE_ENV=development && webpack --profile --json > stats.json",
    "compile": "set NODE_ENV=production && webpack --mode=production",
    "db:setup": "node database-mongodb/seed.js",
    "start-dev": "set NODE_ENV=development && npm run compile && nodemon server.js",
    "start": "set NODE_ENV=production && node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FiveFinance/ratings_history_module.git"
